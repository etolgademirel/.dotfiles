alias ls="ls --color=auto -h"
alias ll="ls -alF"
alias la="ls -A"
 
alias mkdir="mkdir -pv"
 
alias rm="rm -v"
alias rmf="rm -rfvI"

alias grep="grep --color=auto -i"
alias grepr="grep --color=auto -IHrni"
alias hgrep="history | grep"
 
alias .1="cd .."
alias .2="cd ../.."
alias .3="cd ../../.."
alias .4="cd ../../../.."
alias .5="cd ../../../../.."
 
alias c="clear"
alias cl="clear"
alias clr="clear"
